rl } }) => (\n                <Post\n                    key={id}\n                    name={name}\n                    description={description}\n                    message={message}\n                    photoUrl={ photoUrl}\n                />\n            ))}\n         </FlipMove>\n        </div>\n    )\n}\n\nexport default Feed;\n","/Users/arjan/Sites/linkedin-clone/src/InputOption.js",[],"/Users/arjan/Sites/linkedin-clone/src/Post.js",[],"/Users/arjan/Sites/linkedin-clone/src/firebase.js",[],"/Users/arjan/Sites/linkedin-clone/src/features/userSlice.js",[],"/Users/arjan/Sites/linkedin-clone/src/Login.js",[],"/Users/arjan/Sites/linkedin-clone/src/Widget.js",[],{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":29,"column":6,"nodeType":"71","endLine":29,"endColumn":8,"suggestions":"72"},{"ruleId":"73","severity":1,"message":"74","line":1,"column":10,"nodeType":"75","messageId":"76","endLine":1,"endColumn":16},"no-native-reassign",["77"],"no-negated-in-lhs",["78"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.","ArrayExpression",["79"],"no-unused-vars","'Create' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation",{"desc":"80","fix":"81"},"Update the dependencies array to be: [dispatch]",{"range":"82","text":"83"},[822,824],"[dispatch]"]